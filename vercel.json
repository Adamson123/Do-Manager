{
  "functions": {
    "api/updateProfileAction.js": {
      "includeFiles": "public/images/**/*"
    },
    "src/actions/*.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/src/actions/:path*"
    }
  ]
}
